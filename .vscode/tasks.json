{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "npm: dev",
			"type": "npm",
			"script": "dev",
			"isBackground": true,
			"problemMatcher": {
				"owner": "custom",
				"pattern": {
					"regexp": "^([^\\s].*)\\((\\d+|\\d+,\\d+|\\d+,\\d+,\\d+,\\d+)\\):\\s+(error|warning|info)\\s+(TS\\d+)\\s*:\\s*(.*)$",
					"file": 1,
					"location": 2,
					"severity": 3,
					"code": 4,
					"message": 5
				},
				"background": {
					"activeOnStart": true,
					"beginsPattern": "^.*Local:.*$",
					"endsPattern": "^.*ready in.*$"
				}
			}
		},
		{
			"label": "npm: build",
			"type": "npm",
			"script": "build",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": [
				"$tsc"
			]
		},
		{
			"label": "npm: preview",
			"type": "npm",
			"script": "preview",
			"isBackground": true
		}
	]
}